{
	"name": "bmidataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "bmimean_input",
						"type": "DatasetReference"
					},
					"name": "bmimeaninput"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filter",
					"description": "Filtering my country income group"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"parameters{",
				"      as string",
				"}",
				"source(output(",
				"          Id as string,",
				"          IndicatorCode as string,",
				"          SpatialDimension as string,",
				"          WorldBankIncomeGroup as string,",
				"          TimeDimension as string,",
				"          TimeDim as string,",
				"          DisaggregatingDimension1 as string,",
				"          Sex as string,",
				"          DisaggregatingDimension2 as string,",
				"          DisaggregatingDimension2ValueCode as string,",
				"          DisaggregatingDimension3 as string,",
				"          DisaggregatingDimension3ValueCode as string,",
				"          DataSourceDimension as string,",
				"          DataSourceDimensionValueCode as string,",
				"          Value as string,",
				"          Prevalence as string,",
				"          Low as string,",
				"          High as string,",
				"          Comments as string,",
				"          Date as string,",
				"          TimeDimensionValue as string,",
				"          TimeDimensionBegin as string,",
				"          TimeDimensionEnd as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> bmimeaninput",
				"select1 filter(WorldBankIncomeGroup['HighIncome']==TRUE) ~> filter",
				"bmimeaninput select(mapColumn(",
				"          Id,",
				"          IndicatorCode,",
				"          SpatialDimension,",
				"          WorldBankIncomeGroup,",
				"          TimeDimension,",
				"          Year = TimeDim,",
				"          DisaggregatingDimension1,",
				"          Sex,",
				"          DisaggregatingDimension2,",
				"          DisaggregatingDimension2ValueCode,",
				"          DisaggregatingDimension3,",
				"          DisaggregatingDimension3ValueCode,",
				"          DataSourceDimension,",
				"          DataSourceDimensionValueCode,",
				"          Value,",
				"          Prevalence,",
				"          Low,",
				"          High,",
				"          Comments,",
				"          Date,",
				"          TimeDimensionValue,",
				"          TimeDimensionBegin,",
				"          TimeDimensionEnd",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}